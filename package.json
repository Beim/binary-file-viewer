{
    "name": "binary-file-viewer",
    "displayName": "Binary File Viewer",
    "description": "A viewer for binary files. The viewer can be customized to interprete the contents of any binary file.",
    "version": "0.1.0",
    "publisher": "maziac",
    "author": {
        "name": "Thomas Busse"
    },
    "license": "MIT",
    "keywords": [
        "binary",
        "viewer",
        "customizable"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/maziac/binar-file-viewer"
    },
    "engines": {
        "vscode": "^1.63.0"
    },
    "icon": "assets/icon.png",
    "categories": [
        "Visualization"
    ],
    "activationEvents": [
        "onCustomEditor:binary-file-viewer.viewer",
        "onCommand:binary-file-viewer.open"
    ],
    "capabilities": {
        "untrustedWorkspaces": {
            "supported": true
        }
    },
    "main": "./out/extension",
    "contributes": {
        "customEditors": [
            {
                "viewType": "binary-file-viewer.viewer",
                "displayName": "Binary File Viewer",
                "selector": [
                    {
                        "filenamePattern": "*.obj*"
                    },
                    {
                        "filenamePattern": "*.wav"
                    }
                ],
                "priority": "default"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "binary-file-viewerbiary configuration",
            "properties": {
                "binary-file-viewer.parserGlobPatterns": {
                    "scope": "window",
                    "type": "array",
                    "default": [],
                    "markdownDescription": "Add here the path(s) to your parser files. You can not only add a path but a full glob pattern. The Binary-File-Viewer will observe these locations. If a file is changed (on disk) it is automatically used for parsing. normally you need to add absolute paths. You can do so e.g. by right-click on the folder in vscode, copy the path and paste it here. Complete it with the globbing, e.g. '/*.js'. E.g. '/Volumes/binary-file-viewer-examples/parsers/*.js'",
                    "items": {
                        "type": "string"
                    }
                }
            }
        }
    },
    "scripts": {
        "test-compile": "tsc -p ./",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^17.0.0",
        "@types/vscode": "^1.63.0"
    },
    "dependencies": {
        "chart.js": "^3.6.2",
        "chartjs-plugin-zoom": "^1.2.0",
        "gif-writer": "^0.9.4",
        "hammerjs": "^2.0.8",
        "nsfw": "^2.1.2",
        "path": "^0.12.7"
    }
}
