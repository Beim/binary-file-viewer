{
    "name": "binary-file-viewer",
    "displayName": "Binary File Viewer",
    "description": "A viewer for binary files. The viewer can be customized to interprete the contents of any binary file.",
    "version": "0.1.0",
    "publisher": "maziac",
    "author": {
        "name": "Thomas Busse"
    },
    "license": "MIT",
    "keywords": [
        "binary",
        "viewer",
        "customizable"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/maziac/binar-file-viewer"
    },
    "engines": {
        "vscode": "^1.63.0"
    },
    "icon": "assets/icon.png",
    "categories": [
        "Visualization"
    ],
    "activationEvents": [
        "onCustomEditor:binary-file-viewer.viewer",
        "onCommand:binary-file-viewer.open"
    ],
    "capabilities": {
        "untrustedWorkspaces": {
            "supported": true
        }
    },
    "main": "./out/extension",
    "contributes": {
        "customEditors": [
            {
                "viewType": "binary-file-viewer.viewer",
                "displayName": "Binary File Viewer",
                "selector": [
                    {
                        "filenamePattern": "*.*"
                    }
                ],
                "priority": "default"
            }
        ]
    },
    "scripts": {
        "test-compile": "tsc -p ./",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^17.0.0",
        "@types/vscode": "^1.63.1"
    },
    "dependencies": {
        "gif-writer": "^0.9.4",
        "nsfw": "^2.1.2",
        "path": "^0.12.7"
    }
}
